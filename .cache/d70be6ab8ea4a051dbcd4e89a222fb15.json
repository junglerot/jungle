mn":15}},{"generated":{"line":161,"column":31},"source":"js/components/Props.js","original":{"line":107,"column":4}},{"generated":{"line":161,"column":33},"source":"js/components/Props.js","original":{"line":107,"column":30}},{"generated":{"line":161,"column":46},"source":"js/components/Props.js","original":{"line":107,"column":40}},{"generated":{"line":161,"column":50},"source":"js/components/Props.js","original":{"line":107,"column":4}},{"generated":{"line":162,"column":0},"source":"js/components/Props.js","original":{"line":108,"column":6}},{"generated":{"line":163,"column":0},"source":"js/components/Props.js","original":{"line":108,"column":6}},{"generated":{"line":164,"column":0},"source":"js/components/Props.js","original":{"line":108,"column":6}},{"generated":{"line":165,"column":0},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":166,"column":0},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":167,"column":0},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":167,"column":12},"source":"js/components/Props.js","original":{"line":109,"column":14}},{"generated":{"line":167,"column":21},"source":"js/components/Props.js","original":{"line":109,"column":20}},{"generated":{"line":167,"column":30},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":168,"column":0},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":169,"column":0},"source":"js/components/Props.js","original":{"line":109,"column":8}},{"generated":{"line":170,"column":0},"source":"js/components/Props.js","original":{"line":108,"column":6}},{"generated":{"line":171,"column":0},"source":"js/components/Props.js","original":{"line":107,"column":4}},{"generated":{"line":172,"column":0},"source":"js/components/Props.js","original":{"line":5,"column":2}},{"generated":{"line":172,"column":3},"source":"js/components/Props.js","original":{"line":4,"column":20}},{"generated":{"line":173,"column":0},"source":"js/components/Props.js","original":{"line":4,"column":20}},{"generated":{"line":173,"column":1},"source":"js/components/Props.js","original":{"line":4,"column":7}}],"sources":{"js/components/Props.js":"import { h } from 'hyperapp'\nimport { emoji } from '../utils'\n\nexport const view = ({ state, actions }) => (\n  <section class=\"section\" id=\"props\">\n    <div class=\"section__icon-wrapper\" innerHTML={emoji('üè∑Ô∏è')} />\n    <div class=\"section__heading-wrapper\">\n      <a class=\"section__heading\" href=\"#props\">\n        Props\n      </a>\n    </div>\n    <p>\n      It's important to distinguish between the object returned from calling tippy() and a Tippy\n      instance. When you call tippy(), it can create multiple tooltips (Tippy instances) at once.\n    </p>\n    <p>\n      Tippy instances refer to individual tooltips, whereas the object returned from tippy() refers\n      to the collection.\n    </p>\n\n    <h3>\n      <code>tippy()</code> object\n    </h3>\n    <div class=\"code-wrapper\" data-lang=\"js\">\n      <pre>\n        <code class=\"lang-js\">{`const tipObj = tippy('.btn')`}</code>\n      </pre>\n    </div>\n    <p>\n      <code>tipObj</code> is a plain object.\n    </p>\n    <div class=\"code-wrapper\" data-lang=\"js\">\n      <pre>\n        <code class=\"lang-js\">{`{\n  // selector that was supplied to tippy()\n  selector: '.btn', \n\n  // default + instance options merged together\n  options: { ... }, \n\n  // Array of all Tippy instances that were created\n  tooltips: [Tippy, Tippy, Tippy, ...], \n\n  // Method to destroy all the tooltips that were created\n  destroyAll() { }\n}`}</code>\n      </pre>\n    </div>\n\n    <h3>Tippy instances</h3>\n    <p>\n      Stored on reference elements via the <code>_tippy</code> property, and inside the{' '}\n      <code>tooltips</code> array of the <code>tippy()</code> object.\n    </p>\n    <div class=\"code-wrapper\" data-lang=\"js\">\n      <pre>\n        <code class=\"lang-js\">{`tippy('.btn')\nconst btn = document.querySelector('.btn')\nconst tipInstance = btn._tippy`}</code>\n      </pre>\n    </div>\n    <p>\n      <code>tipInstance</code> is a Tippy instance.\n    </p>\n    <div class=\"code-wrapper\" data-lang=\"js\">\n      <pre>\n        <code class=\"lang-js\">\n          {`{\n  // id of the Tippy instance (1 to Infinity)\n  id: 1,\n\n  // Popper element that contains the tooltip\n  popper: Element,\n\n  // Popper instance is not created until shown for the first time,\n  // unless specified otherwise\n  popperInstance: null,\n\n  // Reference element that is the trigger for the tooltip\n  reference: Element,\n\n  // Array of objects containing the event + handler function of each trigger\n  listeners: [{ ... }, { ... }, ...],\n\n  // Defaults + instance + attribute options merged together\n  options: { ... },\n\n  // The state of the tooltip\n  state: {\n    // Has the instance been destroyed?\n    destroyed: false,\n    // Is the instance enabled?\n    enabled: true,\n    // Is the tooltip currently visible and not transitioning out?\n    visible: false\n  },\n\n  // title content of the tooltip (null if HTML)\n  title: 'example'\n}`}\n        </code>\n      </pre>\n    </div>\n\n    <h3>Shortcuts</h3>\n    <p>There are several shortcuts available for accessing the instance.</p>\n    <div class=\"code-wrapper\" data-lang=\"js\">\n      <pre>\n        <code class=\"lang-js\">{`// The popper element has the instance attached to it:\npopper._tippy\n// As does the reference element (as seen above):\nreference._tippy\n// The popper also has the reference directly attached:\npopper._reference`}</code>\n      </pre>\n    </div>\n  </section>\n)\n"},"lineCount":null}},"hash":"e3f2db23f8ffa0a08e7b43a11f92e4d5","cacheData":{"env":{}}}